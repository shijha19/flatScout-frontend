# Frontend Environment Variables for Netlify
# Copy this file to .env.production and fill in your production values

# Backend API URL (will be your Render app URL)
VITE_API_URL=https://your-backend-app.onrender.com

# Stream Chat Configuration
VITE_STREAM_API_KEY=your-stream-api-key

# Google Maps API Key (if using Google Maps)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Other environment variables as needed
VITE_APP_NAME=FlatScout
